# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'College Fair' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for College Fair
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Auth'
  pod 'Firebase/Storage'

  target 'College FairTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'College FairUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'College Fair College' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for College Fair College
  pod 'Firebase/Core'
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Auth'
  pod 'Firebase/Storage'

end
